package test;

/**
 * test
 */
public class test {

   public static void main(String[] args) {
      Rect good = new Rect();
      good.tenPlusEight(5, 5);
      int area = good.tenPlusEight(5, 6);
      System.out.println(area);
   }
}
class Rect{
public int tenPlusEight(int m,int n) {
   for (int i = 0; i < m; i++) {
      for (int j = 0; j < n; j++) {
         System.out.print("*");
      }
      System.out.println();
  }
   return m*n;
}

}
